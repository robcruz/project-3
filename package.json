    "license": "ISC",
    "dependencies": {
        "babel-preset-env": "^1.7.0",
        "body-parser": "^1.18.3",
        "concurrently": "^3.6.0",
        "cors": "^2.8.4",
        "express": "^4.17.1",
        "helmet": "^3.12.1",
        "mongoose": "^5.7.5",
        "prop-types": "^15.7.2",
        "react-avatar": "^3.6.0",
        "react-scripts": "^3.4.1",
        "socket.io": "^2.4.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "nodemon": "^1.17.5"
    }
}
